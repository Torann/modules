{
    "name": "torann/modules",
    "description": "Basic module implantation for Laravel",
    "authors": [
        {
            "name": "Daniel Stainback",
            "email": "torann@gmail.com"
        }
    ],
    "keywords": [
        "modules",
        "laravel modules",
        "modular"
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/support": "~5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Torann\\Modules\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Torann\\Modules\\Tests\\": "tests/"
        }
    }
}
